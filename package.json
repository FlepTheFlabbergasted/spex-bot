  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "start": "node index.js",
    "register:dev": "cross-env GUILD_DEV_ID=369422543712550912 node commands/util/register-commands.js",
