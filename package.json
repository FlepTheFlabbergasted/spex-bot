    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "discord-interactions": "^4.1.1",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0"
  }
